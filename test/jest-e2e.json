{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@app/lib/(.*)": "<rootDir>/../libs/lib/src/$1",
    "@app/lib": "<rootDir>/../libs/lib/src",
    "@app/propelauth/(.*)": "<rootDir>/../libs/propelauth/src/$1",
    "@app/propelauth": "<rootDir>/../libs/propelauth/src",
    "@app/kysely-pg/(.*)": "<rootDir>/../libs/kysely-pg/src/$1",
    "@app/kysely-pg": "<rootDir>/../libs/kysely-pg/src",
    "@app/filestack/(.*)": "<rootDir>/../libs/filestack/src/$1",
    "@app/filestack": "<rootDir>/../libs/filestack/src",
    "@app/postgresjs/(.*)": "<rootDir>/../libs/postgresjs/src/$1",
    "@app/postgresjs": "<rootDir>/../libs/postgresjs/src"
  }
}